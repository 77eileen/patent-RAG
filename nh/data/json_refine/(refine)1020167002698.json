        },
        {
          "claim_number": 24,
          "claim_type": "dependent",
          "change_type": "신규",
          "change_code": "I",
          "text": "제17항에 있어서, 아미노산이 이소류신인 방법.",
          "refers_to": [
            17
          ]
        }
      ]
